  "CREATE PROCEDURE statement (SQL - external)": {
    "prefix": "CREATE PROCEDURE statement (SQL - external)",
    "body": [
      "${LINE_COMMENT} ####################################################################",
      "${LINE_COMMENT} # Basic CREATE PROCEDURE statement (SQL - external)",
      "${LINE_COMMENT} # See https://www.ibm.com/docs/en/db2-for-zos/13?topic=statements-create-procedure-sql-external-deprecated for complete syntax.",
      "${LINE_COMMENT} ####################################################################",
      "--#SET TERMINATOR #",
      "CREATE PROCEDURE ${1:procedure-name} ()",
      "\tRESULT SETS 1",
      "\tLANGUAGE SQL",
      "\tFENCED",
      "\tCOLLID TEST",
      "\tWLM ENVIRONMENT WLMENV1",
      "\tRUN OPTIONS 'NOTEST(NONE,*,*,*)'",
      "P1: BEGIN",
      "\t${LINE_COMMENT} Declare cursor",
      "\tDECLARE cursor1 CURSOR WITH RETURN FOR",
      "\t${LINE_COMMENT} #######################################################################",
      "\t${LINE_COMMENT} # Replace the SQL statement with your statement.",
      "\t${LINE_COMMENT} #  Note: Be sure to end statements with the terminator character",
      "\t${LINE_COMMENT} #  (usually ';')",
      "\t${LINE_COMMENT} # The example SQL statement SELECT NAME FROM SYSIBM.SYSTABLES",
      "\t${LINE_COMMENT} # returns all table names from SYSIBM.SYSTABLES.",
      "\t${LINE_COMMENT} ######################################################################",
      "\t\tSELECT NAME FROM SYSIBM.SYSTABLES;",
      "\t",
      "\t${LINE_COMMENT} Cursor left open for client application",
      "\tOPEN cursor1;",
      "END P1#",
      "--#SET TERMINATOR ;"
    ],
    "description": "Sample CREATE PROCEDURE statement (SQL - external)"
  },
  "CREATE PROCEDURE statement (SQL - native)": {
    "prefix": "CREATE PROCEDURE statement (SQL - native)",
    "body": [
      "${LINE_COMMENT} ####################################################################",
      "${LINE_COMMENT} # Basic CREATE PROCEDURE statement (SQL - native)",
      "${LINE_COMMENT} # See https://www.ibm.com/docs/en/db2-for-zos/13?topic=statements-create-procedure-sql-native for complete syntax.",
      "${LINE_COMMENT} ####################################################################",
      "--#SET TERMINATOR #",
      "CREATE PROCEDURE ${1:procedure-name} ( VARNAME VARCHAR(128) )",
      "\tVERSION V1",
      "\tISOLATION LEVEL CS",
      "\tRESULT SETS 1",
      "\tLANGUAGE SQL",
      "P1: BEGIN",
      "\t${LINE_COMMENT} #######################################################################",
      "\t${LINE_COMMENT} # Returns all tables created by VARNAME and CARDF greater than VARCOUNT",
      "\t${LINE_COMMENT} #######################################################################",
      "\tDECLARE VARCOUNT INTEGER DEFAULT 10000;",
      "\t${LINE_COMMENT} Declare cursor",
      "\tDECLARE cursor1 CURSOR WITH RETURN FOR",
      "\t\tSELECT NAME",
      "\t\tFROM SYSIBM.SYSTABLES",
      "\t\tWHERE CREATOR = VARNAME",
      "\t\t\tAND CARDF > VARCOUNT",
      "\t\tORDER BY NAME;",
      "\t",
      "\t${LINE_COMMENT} Cursor left open for client application",
      "\tOPEN cursor1;",
      "END P1#",
      "--#SET TERMINATOR ;"
    ],
    "description": "Sample CREATE PROCEDURE statement (SQL - native)"
  },
