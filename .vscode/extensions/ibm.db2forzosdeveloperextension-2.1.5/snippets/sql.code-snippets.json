  "CREATE FUNCTION statement (compiled SQL scalar)": {
    "prefix": "CREATE FUNCTION statement (compiled SQL scalar)",
    "body": [
      "${LINE_COMMENT} ####################################################################",
      "${LINE_COMMENT} # Basic CREATE FUNCTION statement (compiled SQL scalar)",
      "${LINE_COMMENT} # See https://www.ibm.com/docs/en/db2-for-zos/13?topic=statements-create-function-compiled-sql-scalar for complete syntax.",
      "${LINE_COMMENT} ####################################################################",
      "--#SET TERMINATOR #",
      "CREATE FUNCTION ${1:function-name} ( VARNAME VARCHAR(128) )",
      "\tRETURNS INTEGER",
      "\tVERSION V1",
      "\tDETERMINISTIC",
      "\tNO EXTERNAL ACTION",
      "\tREADS SQL DATA",
      "BEGIN",
      "\t${LINE_COMMENT} ##############################################################",
      "\t${LINE_COMMENT} # Returns the number of tables created by SYSIBM and NAME like VARNAME",
      "\t${LINE_COMMENT} ##############################################################",
      "\tDECLARE VARCOUNT INTEGER;",
      "\tSELECT COUNT(*) INTO VARCOUNT",
      "\tFROM SYSIBM.SYSTABLES",
      "\tWHERE CREATOR = 'SYSIBM'",
      "\tAND NAME LIKE VARNAME;",
      "\tRETURN (VARCOUNT);",
      "END#",
      "--#SET TERMINATOR ;"
    ],
    "description": "Sample CREATE FUNCTION statement (compiled SQL scalar)"
  },
